    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Windows Debug x64",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/godot/bin/godot.windows.tools.64.exe",
            "args": ["-e"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "preLaunchTask": "Build Debug Windows x64 Godot"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}